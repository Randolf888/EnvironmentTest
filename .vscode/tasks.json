{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Start Metro",
      "type": "process",
      "command": "npm",
      "args": ["start"],
      "group": "build",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "new"
      },
      "isBackground": true
